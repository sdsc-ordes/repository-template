[package]
  name = "{{ library_name }}"
  version = '{{ project_version }}'
  description = "{{ project_description }}"
  license = "{{ project_license }}"
  homepage = " {{ project_url }} "
  keywords = []
  authors = [
{%- for a in project_authors %}
  "{{ a.first_name }} {{ a.last_name }} <{{ a.email }}>",
{%- endfor %}
  ]
  edition = "2021"

[dependencies]
  anyhow = "1.0.93"

[lints.clippy]
  pedantic = "deny"
  needless_return = "warn"
